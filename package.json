{
  "version": "0.0.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "vitest",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "coverage": "vitest run --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@mariolazzari/met",
  "author": "Mario Lazzari",
  "module": "dist/met.esm.js",
  "size-limit": [
    {
      "path": "dist/met.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/met.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@vitest/coverage-v8": "^1.6.0",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "met",
    "museum",
    "metropolitan",
    "new york",
    "art",
    "nodejs",
    "typescript",
    "vitest",
    "tsdx",
    "rest"
  ]
}
